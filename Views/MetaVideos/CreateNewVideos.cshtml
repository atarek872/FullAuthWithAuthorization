<section class="fun_fact_area fun_fact_area_two Fun_fa_fse">
    <div class="container">
        <div class="row flex-row-reverse">
            <div class="col-lg-7 col-md-6">
                <div class="fact_author_img fact_author_img_two text-right">
                    <div class="square_box box_three"></div>
                    <div class="square_box box_four"></div>
                    <img class="wow fadeInUp" data-wow-delay="0.3s" src="~/img/home-tracking/tracking_banner_img.png" alt=""
                         style="visibility: visible; animation-delay: 0.3s; animation-name: fadeInUp;">
                </div>
            </div>
            <div class="col-lg-5 col-md-6">
                <div class="fun_fact_content">
                    <h1 class="t_color3 f_p f_700 mb_20">Create new meta</h1>
                    <p class="f_400 f_size_15 l_height28 mb_40">to create new video ,Just you need 100 Point.</p>
                    <div class="d-flex">
                        <div class="fact_item">
                            <h2 class="t_color3">MY BOINTS</h2>
                            <h4 class="mb-0"><span id="unit">@Model.bonus.TotalBouns</span> Unit</h4>
                            <h4 class="mb-0"><span id="dollar">@Model.Dollar</span> $</h4>
                            <p class="f_400 f_size_15 l_height28 mb_40">You can collect your money if you reach 20$</p>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>



<section class="tracking_banner_area text-center">
    <div class="container">
        <div class="tracking_banner_content">
            <h3 class="wow fadeInUp" data-wow-delay="0.8s">Spend less <strong>time tracking</strong> </h3>
            <p class="wow fadeInUp" data-wow-delay="0.9s">Track your time and watch your productivity soar!</p>
            @* add vids Form *@
            <div class="wow fadeInUp" data-wow-delay="0.9s">
                <form method="post" class="login-form mt_60">
                    <div class="form-group text_box">
                        <label asp-for="IframUri" class="f_p text_c f_400">IFrameURI (Copy it from Youtube video)</label>
                        <input asp-for="IframUri" type="url" placeholder="https://youtu.be/hRjzu">
                    </div>


                    <div style="color:#ff006e">@Html.ValidationMessage("alreadyExist")</div>
                    <div style="color:#ff006e">@Html.ValidationMessage("NoBonus")</div>
                    <div style="color:#ff00dc">@TempData["Message"]</div>


                    <button type="submit" class="agency_banner_btn">Add Video</button>
                </form>
            </div>
            <br /><br />
            <img class="wow fadeIn" data-wow-delay="0.9s" src="~/img/home-tracking/tracking_banner_img.png"
                 alt="">
        </div>
        <div class="row tracking_software_logo">
            <div class="col-lg-2 col-md-3 col-4">
                <a href="#"><img src="~/img/home-tracking/apple.png" alt=""></a>
            </div>
            <div class="col-lg-1 col-md-3 col-4">
                <a href="#"><img src="~/img/home-tracking/windows.png" alt=""></a>
            </div>
            <div class="col-lg-1 col-md-3 col-4">
                <a href="#"><img src="~/img/home-tracking/google.png" alt=""></a>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-4 col-6">
                <a href="#"><img src="~/img/home-tracking/tesla.png" alt=""></a>
            </div>
            <div class="col-lg-3 col-md-3 col-sm-4 col-6">
                <a href="#"><img src="~/img/home-tracking/amazon.png" alt=""></a>
            </div>
            <div class="col-lg-2 col-md-3 col-sm-4 col-6">
                <a href="#"><img src="~/img/home-tracking/aliexpress.png" alt=""></a>
            </div>
        </div>
    </div>
</section>
<section class="tracking_activity_area">
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">My Videos</th>
                    <th scope="col">URL</th>
                    <th scope="col">Status</th>
                    <th scope="col">Views On It</th>
                    <th scope="col">Bouns From It</th>
                    <th scope="col">Duration Time</th>
                    <th scope="col">Uploaded at</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.Myownvids)
                {
                    <tr>
                        <th scope="row">-</th>
                        @foreach (var item2 in Model.Video)
                        {
                            @if (item2.id == item.VideoId)
                            {
                                <td>@item2.IframUri</td>
                                @if (@item2.status == 1)
                                {

                                    <td>Approved</td>
                                }
                                else
                                {
                                    <td>Pending</td>

                                }


                            }

                        }

                        <td>@item.ViewsOnIt</td>
                        <td>@item.EarnBounsFromIt</td>
                        <td>@item.DurationTime</td>
                        <td>@item.UploadedAt</td>
                    </tr>
                }

                @*<tr>
                        <th scope="row"></th>
                        <td>https://youtu.be/nIY6VxBKgaU</td>
                        <td>0</td>
                        <td>0</td>
                        <td>2300 sec</td>
                        <td>2022-01-26 00:45:44.807</td>
                    </tr>*@

            </tbody>
        </table>
    </div>
</section>
